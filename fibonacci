#include "stdio.h"
 
void main(){
    
    int f1 = 0, f2 = 1, f3, contador;
    printf("Digite um valor inteiro: ");
    scanf("%d", &contador);
    
    if(contador > 0){
        
    
        printf("Fibonacci: ");
        printf("%d, %d, ", f1, f2); // 2 primeiros valores
    
        for(int i = 0; i < contador-2; i++){ //vai rodar 8 vezes
        
            f3 = f1 + f2;
            f1 = f2;
            f2 = f3;
        
            printf("%d, ", f3);
            //codigo;
     
        }
        
    }
    
    else{
        printf("Número inválido");
    }
}
